<%- include('partials/header') %>


<form action="/dashboard" method="get" >
    <button type="submit" class="button-a dv-b" >Home</button>
</form>

<h1 class="wel-msg"> <a href="/getProfile/<%=page.auther_id%>" class="dt-b-a">Welcome to <%= page.author_name %>'s page </a> </h1>
<hr class="hr-profile-dash-board" />

<div class="editor-compose" >
    <form class="" action="/editPage" method="post">
        <input hidden value="<%= page.id %>" name="pageId">
        <div class="editor-text">
        <textarea name="content" id="Content" class="form-control" rows="30" cols="40"><%= page.content %></textarea>
            <br/>
        </div>
        <button type="submit" class="button-a but-tp" name="button"> Submit </button>
    </form>
</div>

<script>tinymce.init({selector:'textarea'});</script>
<%- include('partials/footer') %>
